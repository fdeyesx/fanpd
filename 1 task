#include <iostream>
using namespace std;

int main() {    
  cout << "Enter number (integer): ";
  int num;
  cin >> num;

  if ( num % 3 == 0 && num % 5 == 0 && num % 15 ==0) {
    cout << "FizzBuzzFizzBuzz";
  } else if (num % 5 == 0 && num % 15 == 0) {
    cout << "FizzBuzzBuzz" ;
  } else if ( num % 3 == 0) {
    cout << "Fizz" << endl;
  } else if ( num % 5 == 0) {
    cout << "Buzz" << endl;
  } else if ( num % 15 == 0) {
    cout << "FizzBuzz" << endl;
  } else {
    cout << "This number isn't multiple to 3, 5, 15." << endl;
  }

    return 0;
}
